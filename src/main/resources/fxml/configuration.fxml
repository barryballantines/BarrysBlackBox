<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="191.0" prefWidth="367.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Flightgear HTTPD" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label layoutX="15.0" layoutY="46.0" text="Hostname" AnchorPane.leftAnchor="14.0" />
      <Label layoutX="15.0" layoutY="77.0" text="Port" AnchorPane.leftAnchor="14.0" />
      <TextField fx:id="fgHostnameTxt" layoutX="79.0" layoutY="42.0" onAction="#flightgearConnectionChanged" promptText="localhost" text="localhost" AnchorPane.leftAnchor="79.0" AnchorPane.rightAnchor="139.0" AnchorPane.topAnchor="42.0" />
      <TextField fx:id="fgPortTxt" layoutX="79.0" layoutY="73.0" onAction="#flightgearConnectionChanged" prefHeight="29.0" prefWidth="49.0" promptText="5500" text="5500" AnchorPane.leftAnchor="79.0" AnchorPane.topAnchor="73.0" />
      <Button layoutX="240.0" layoutY="42.0" mnemonicParsing="false" onAction="#testFlightgearConnection" prefHeight="25.0" prefWidth="119.0" text="Test Connection" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="42.0" />
      <Label fx:id="fgTestFeedbackLbl" layoutX="135.0" layoutY="77.0" prefHeight="19.0" prefWidth="217.0" text="Press button to test connection" AnchorPane.leftAnchor="135.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="77.0" />
      <Label layoutX="14.0" layoutY="120.0" text="UDP Server (experimental)" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="120.0">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <CheckBox fx:id="udpServerRunningCheck" layoutX="14.0" layoutY="146.0" mnemonicParsing="false" onAction="#udpRunningStateChanged" text="Server not running" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="146.0" />
      <TextField fx:id="udpPortText" layoutX="303.0" layoutY="141.0" prefHeight="29.0" prefWidth="49.0" promptText="5550" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="141.0" onAction="#udpServerPortChanged" />
      <Label layoutX="223.0" layoutY="146.0" prefHeight="19.0" prefWidth="73.0" text="Server Port:" AnchorPane.rightAnchor="71.0" AnchorPane.topAnchor="146.0" />
   </children>
</AnchorPane>
